{
  "team": {
    "name": "Netherton United",
    "shortName": "Netherton",
    "abbreviation": "NUGT",
    "defaultOpponentName": "Opposition"
  },
  "branding": {
    "primaryColor": "#dc3545",
    "secondaryColor": "#ffffff",
    "logoUrl": "./nugtlogo.png",
    "faviconUrl": "./favicon.ico",
    "appIconUrl": "./nugt512.png"
  },
  "pwa": {
    "appName": "Netherton United Game Time App",
    "shortName": "NUGT",
    "description": "Advanced football match tracker with attendance, statistics, and enhanced event management",
    "themeColor": "#dc3545",
    "backgroundColor": "#ffffff"
  },
  "integrations": {
    "leagueTable": {
      "enabled": true,
      "defaultUrl": "https://example-league-url.com",
      "corsProxies": [
        "https://corsproxy.io/?",
        "https://cors-anywhere.herokuapp.com/",
        "https://api.allorigins.win/get?url="
      ]
    },
    "statistics": {
      "enabled": true,
      "apiEndpoint": null
    }
  },
  "defaults": {
    "matchDuration": 4200,
    "theme": "red",
    "darkMode": false
  },
  "storage": {
    "keyPrefix": "nugt_",
    "cachePrefix": "nugt-cache-"
  }
}